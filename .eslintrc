{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/vue3-essential",
    "eslint:recommended",
    "@vue/eslint-config-typescript"
  ],
  "rules": {
    "vue/multi-word-component-names": "off",
    "vue/attributes-order": ["error", { "alphabetical": true }],
    "vue/valid-template-root": "off"
  }
}
